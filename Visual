local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()
local Window = Rayfield:CreateWindow({
   Name = "Visual Blade ball [ Revamped ]",
   LoadingTitle = "Visual Blade ball",
   LoadingSubtitle = "by Fsploit",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Arrayfield"
   },
   Discord = {
      Enabled = false,
      Invite = "https://discord.com/invite/DcrzMGKwx2",
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Visual Keysystem",
      Subtitle = "Key System",
      Note = "Join the discord (https://discord.com/invite/DcrzMGKwx2)",
      FileName = "SiriusKey",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "Visual711"
   }
})

